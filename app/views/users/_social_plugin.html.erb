<table>
    <tr>
      <td id="fb" style="padding: 18px 27px 14px 2px;">
        <%= render :partial => "users/facebook" %>     
      </td>
      <td id="tweet" style="padding: 52px 0px 0px;">
        <%= render :partial => "users/tweet" %>     
      </td>
      <td id="website" style="padding: 44px 0px 0px;">
        <%if web_like(params[:id])%>
          <%= render :partial => "users/website_unlike" %>
        <%else%>
          <%= render :partial => "users/website_like" %>
        <%end%>
      </td>
    </tr>
</table>